/*
using System;
using System.Collections.Generic;
using System.Linq;

class Program
{
    static void Main()
    {
        List<int> numbers = new List<int> { 1, 2, 3, 4, 5 };
        int addValue = 10;

        Func<List<int>, int, List<int>> addNumberToList = (list, num) =>
        {
            return list.Select(x => x + num).ToList();
        };

        List<int> result = addNumberToList(numbers, addValue);

        Console.WriteLine("Результат: " + string.Join(", ", result));
    }
}
